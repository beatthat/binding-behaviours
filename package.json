{
  "name": "unity-binding-behaviours",
  "version": "0.0.9",
  "description": "Base classes for components that can Bind (as listeners) to various types of events and have all those bindings cleanly/safely unbound, either with an explicit call to Unbind or when the component is destroyed.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beatthat/unity-binding-behaviours.git"
  },
  "files": [
    "index.js",
    "scripts",
    "src"
  ],
  "author": "larrykirschner@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beatthat/unity-binding-behaviours/issues"
  },
  "homepage": "https://github.com/beatthat/unity-binding-behaviours",
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "install:test": "rm -f *.tgz && npm pack && cd test && npm run clean && npm install"
  },
  "dependencies": {
    "ncp": "^2.0.0",
    "mkdirp": "^0.5.1",
    "unity-bindings": "beatthat/unity-bindings#semver:^0.0.2",
    "unity-properties": "beatthat/unity-properties#semver:^0.0.5",
    "unity-utils": "beatthat/unity-utils#semver:^0.0.4",
    "unity-notifications": "beatthat/unity-notifications#semver:^0.0.9",
    "unity-pools": "beatthat/unity-pools#semver:^0.0.5",
    "unity-common-event-types": "beatthat/unity-common-event-types#semver:^0.0.1",
    "unity-extensions": "beatthat/unity-extensions#semver:^0.0.1",
    "unity-collection-extensions": "beatthat/unity-collection-extensions#semver:^0.0.1"
  }
}
